{
  "name": "HackerEarth-Cart",
  "version": "1.0.0",
  "description": "HackerEarth-Cart",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "build": "webpack --config webpack.config.dev.js",
    "start": "npm-run-all --parallel webpack:run lint:watch jest",
    "start:node": "npm-run-all --parallel open:src lint:watch test:watch",
    "webpack:run": "webpack-dev-server --config webpack.config.dev.js",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:debug": "node --inspect-brk ./node_modules/.bin/jest",
    "format": "prettier --write 'src/**/*.js' 'tools/*.js'"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "babel-jest": "^22.0.6",
    "babel-polyfill": "6.8.0",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "connected-react-router": "^4.2.3",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-redux-form": "^1.16.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "2.2.1",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "^0.28.3",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "jest-styled-components": "^4.9.0",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "1.8.0",
